# Variáveis de Ambiente - Cliente React (Exemplo)

# API Backend
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=http://localhost:5000

# App Config
REACT_APP_NAME="Sistema de Gestão de Restaurante"
REACT_APP_VERSION="1.0.0"

# Environment
NODE_ENV=development

# Build Config
GENERATE_SOURCEMAP=true
ESLINT_NO_DEV_ERRORS=true
FAST_REFRESH=true

# Upload Config
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES="image/jpeg,image/png,image/gif,image/webp"

# Features
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_SOUND=true
REACT_APP_ENABLE_DARK_MODE=true

# Debug
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL="development"
